# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

def shared_config
  use_frameworks!
  pod 'SnapKit'
  pod 'RxSwift'
  pod 'RxCocoa'
end

target 'Hello' do
  shared_config
end

target 'NumberInputField' do
    shared_config
end

target 'LocalizedNumberInputField' do
    shared_config
end

target 'TextAttachmentLab' do
  shared_config
  pod 'Kingfisher', '~> 5.0'
end
