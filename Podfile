# Uncomment the next line to define a global platform for your project
platform :ios, '10.0'

def shared_config
  use_frameworks!
  pod 'SnapKit', '4.0.0'
  pod 'RxSwift', '~> 4.0'
  pod 'RxCocoa', '~> 4.0'
end

target 'Hello' do
  shared_config
end

target 'NumberInputField' do
    shared_config
end

target 'LocalizedNumberInputField' do
    shared_config
end
